log_format main '$remote_addr $http_host $remote_user [$time_local] "$request" '
                '$status $request_time $upstream_response_time $body_bytes_sent "$http_referer" '
                '"$http_user_agent" "$http_x_forwarded_for" '
                '"$request_body"';
access_log  log/nginx/access.log  main;

